{
    "name": "@sencha/ext-react-modern-locale",
    "ext-react-name": "@sencha/ext-react-modern-locale",
    "ext-name": "@sencha/ext-modern-locale",
    "SenchaExtName": "@sencha/ext-modern-locale",
    "SenchaExtReactName": "@sencha/ext-react-modern-locale",
    "version": "7.3.0.55",
    "sencha": {
        "name": "locale",
        "namespace": "Ext",
        "type": "code",
        "alternateName": [
            "ext-locale"
        ],
        "framework": "ext",
        "toolkit": "modern",
        "creator": "Sencha",
        "summary": "Ext JS Modern toolkit locale",
        "detailedDescription": "Ext JS Modern toolkit locale for localization to a local language",
        "version": "7.3.0.55",
        "compatVersion": "7.3.0",
        "format": "1",
        "output": "${workspace.subpkg.prefix}/${package.name}",
        "local": true,
        "build": {
            "dir": "${package.output}"
        },
        "sass": {
            "namespace": "Ext",
            "etc": "${package.dir}/sass/etc/all.scss",
            "var": "${package.dir}/sass/var",
            "src": "${package.dir}/sass/src"
        },
        "classpath": "${package.dir}/src",
        "overrides": "${package.dir}/overrides/${package.locale}"
    }
}
