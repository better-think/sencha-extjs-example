{
    "name": "@sencha/ext-classic-theme-material",
    "ext-react-name": "@sencha/ext-react-classic-theme-material",
    "ext-name": "@sencha/ext-classic-theme-material",
    "SenchaExtName": "@sencha/ext-classic-theme-material",
    "SenchaExtReactName": "@sencha/ext-react-classic-theme-material",
    "version": "7.3.0.55",
    "sencha": {
        "name": "theme-material",
        "namespace": "Ext",
        "type": "theme",
        "extend": "theme-triton",
        "framework": "ext",
        "toolkit": "classic",
        "creator": "Sencha",
        "summary": "Ext JS Material Theme",
        "detailedDescription": "Material Design theme for Classic toolkit with Ripple animation effects for buttons",
        "version": "7.3.0.55",
        "compatVersion": "7.3.0",
        "format": "1",
        "output": "${workspace.subpkg.prefix}/${package.name}",
        "local": true,
        "sass": {
            "namespace": "Ext",
            "etc": "${package.dir}/sass/etc/all.scss",
            "var": "${package.dir}/sass/var",
            "src": "${package.dir}/sass/src"
        },
        "classpath": "${package.dir}/src",
        "overrides": "${package.dir}/overrides",
        "slicer": {
            "js": [
                {
                    "path": "${package.dir}/sass/example/custom.js",
                    "isWidgetManifest": true
                }
            ]
        },
        
        "properties": {
            "skip.pkg": 1
        }
    }
}