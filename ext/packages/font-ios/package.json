{
    "name": "@sencha/ext-font-ios",
    "ext-react-name": "@sencha/ext-react-font-ios",
    "ext-name": "@sencha/ext-font-ios",
    "SenchaExtName": "@sencha/ext-font-ios",
    "SenchaExtReactName": "@sencha/ext-react-font-ios",
    "version": "7.3.0.55",
    "sencha": {
        "name": "font-ios",
        "namespace": "Ext",
        "type": "code",
        "creator": "Sencha",
        "summary": "iOS font icons",
        "detailedDescription": "Font icons used in the modern toolkit iOS theme",
        "version": "7.3.0.55",
        "compatVersion": "7.3.0",
        "format": "1",
        "output": "${framework.dir}/build/packages/${package.name}",
        "local": true,
        "sass" : {
            "namespace": "Ext",
            "etc": "${package.dir}/sass/etc/all.scss",
            "var": "${package.dir}/sass/var",
            "src": [
                "${package.dir}/sass/src",
                "${package.dir}/sass/src/all.scss"
            ]
        },
        "classpath": "${package.dir}/src",
        "overrides": "${package.dir}/overrides"
    }
}
